# BACKEND
# ------------------------------------------------------------------------------------------

# MAKE
# ------------------------------------------------------------------------------------------
PHONY	:=	all
all:

# LINTER, FORMATTER
# ------------------------------------------------------------------------------------------
PHONY	+=	lint
lint:
	npm run lint

PHONY	+=	fmt
fmt:
	npm run format

PHONY	+=	fmt-ck
fmt-ck:
	npm run format-ck

# etc...
# ------------------------------------------------------------------------------------------
.PHONY: $(PHONY)
